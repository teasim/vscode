{
  "name": "@teasim/vscode",
  "version": "0.1.0-alpha.0",
  "description": "a vscode plugin for teasim projects.",
  "author": "Billgo <hi@billgo.me>",
  "bugs": "https://github.com/teasim/vscode/issues",
  "license": "MIT",
  "homepage": "/",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "bun run build",
    "build": "rimraf dist && tsup",
    "start": "bunx kill-port 3000 && STORYBOOK_DISABLE_TELEMETRY=1 NODE_NO_WARNINGS=1 storybook dev -p 3000 --no-version-updates",
    "docs": "bunx rimraf storybook-static && bunx storybook build"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@devjskit/biome-config": "^0.2.1",
    "@devjskit/copy-files": "^2.0.0",
    "@devjskit/cross-env": "^1.0.0",
    "@devjskit/kill-port": "^1.0.0",
    "@devjskit/rimraf": "^1.0.0",
    "@happy-dom/global-registrator": "^20.7.0",
    "@rsbuild/core": "^1.7.3",
    "@rsbuild/plugin-react": "^1.4.5",
    "@rsbuild/plugin-svgr": "^1.3.0",
    "@storybook/addon-links": "^10.2.10",
    "@storybook/addon-themes": "^10.2.10",
    "@storybook/cli": "^10.2.10",
    "@storybook/react": "^10.2.10",
    "@types/bun": "^1.3.9",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/web": "^0.0.338",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "^10.2.10",
    "storybook-react-rsbuild": "^3.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "*",
    "react-dom": "*"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "restricted"
  }
}
